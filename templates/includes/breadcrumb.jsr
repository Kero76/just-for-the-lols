<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
    {{for items}}
        {{if isActive}}
        <li class="breadcrumb-item active" aria-current="page">
            <span class="{{:iconClass}}">{{:label}}</span>
        </li>
        {{else}}
        <li class="breadcrumb-item">
            <a href="{{:url}}"><span class="{{:iconClass}}">{{:label}}</span></a>
        </li>
        {{/if}}
    {{/for}}
    </ol>
</nav>